# AI Agent Assistant Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# Core API Keys (Required)
# =============================================================================

# Google Gemini API Key (Required)
GEMINI_API_KEY=your_gemini_api_key_here

# LangSmith API Key (Required for LangGraph monitoring and tracing)
LANGSMITH_API_KEY=your_langsmith_api_key_here

# LangSmith API Key (Required for LangGraph monitoring and tracing)
LANGSMITH_API_KEY=your_langsmith_api_key_here

# =============================================================================
# Auth0 Configuration (Required for Authentication)
# =============================================================================
AUTH0_DOMAIN=your_auth0_domain.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret
AUTH0_AUDIENCE=your_auth0_api_audience # e.g., https://your-api-identifier

# No other API keys are required for this project.
