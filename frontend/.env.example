# Test user credentials for Playwright E2E tests
VITE_TEST_AUTH_EMAIL=test@example.com
VITE_TEST_AUTH_PASSWORD=testpassword123

# AI Agent Assistant Frontend - Environment Configuration

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_WS_BASE_URL=ws://localhost:8000

# Application Configuration
REACT_APP_APP_NAME=AI Agent Assistant
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_GITHUB_INTEGRATION=true
REACT_APP_ENABLE_SPECIALIZED_AGENTS=true
REACT_APP_ENABLE_ANALYTICS=false

# Debug Configuration
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info
